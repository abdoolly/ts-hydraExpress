{
  "name": "testhydra-service",
  "version": "0.0.1",
  "author": "Abdallah Gamal <abdallah.gamal@audaciousfutures.co>",
  "private": true,
  "scripts": {
    "start": "NODE_ENV=development node -r ts-node/register ./src/app.ts",
    "watch": "nodemon",
    "start:prod": "npm run build && NODE_ENV=production node dist/app.js",
    "build": "tsc",
    "debug": "node-debug testHydra-service.js --debug-brk",
    "test": "jest",
    "lint": "eslint --ext .js,.ts ./ --fix"
  },
  "engines": {
    "node": ">=6.2.1"
  },
  "dependencies": {
    "eslint-config-prettier": "^6.1.0",
    "fwsp-config": "1.1.5",
    "fwsp-logger": "0.3.4",
    "fwsp-server-response": "2.2.6",
    "hydra-express": "1.7.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.1",
    "@types/jest": "^24.0.18",
    "@types/node": "^12.7.2",
    "@types/ramda": "^0.26.21",
    "@typescript-eslint/eslint-plugin": "^2.0.0",
    "@typescript-eslint/parser": "^2.0.0",
    "eslint": "^6.2.2",
    "eslint-plugin-prettier": "^3.1.0",
    "express": "^4.17.1",
    "jest": "^24.9.0",
    "superagent": "3.5.2",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.3.0",
    "typescript": "^3.5.3"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "exec": "npm start",
    "ext": "ts"
  }
}
